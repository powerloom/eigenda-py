[run]
source = src
omit = 
    */tests/*
    */__init__.py
    */grpc/*_pb2.py
    */grpc/*_pb2_grpc.py
    */grpc/*/*_pb2.py
    */grpc/*/*_pb2_grpc.py
    */grpc/*/*/*_pb2.py
    */grpc/*/*/*_pb2_grpc.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if TYPE_CHECKING:
    @abstractmethod